<h1>Listing agendas</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Starttime</th>
    <th>Endtime</th>
    <th>Duration</th>
    <th>Agenda</th>
    <th>Details</th>
    <th>Clientparticipants</th>
    <th>Tcsparticipants</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agendas.each do |agenda| %>
  <tr>
    <td><%= agenda.Date %></td>
    <td><%= agenda.StartTime %></td>
    <td><%= agenda.EndTime %></td>
    <td><%= agenda.Duration %></td>
    <td><%= agenda.Agenda %></td>
    <td><%= agenda.Details %></td>
    <td><%= agenda.ClientParticipants %></td>
    <td><%= agenda.TCSParticipants %></td>
    <td><%= link_to 'Show', agenda %></td>
    <td><%= link_to 'Edit', edit_agenda_path(agenda) %></td>
    <td><%= link_to 'Destroy', agenda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Agenda', new_agenda_path %>
